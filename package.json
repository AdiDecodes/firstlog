{
  "name": "firstlog",
  "version": "0.0.1",
  "description": "Advanced Express middleware logger with UUID, GeoIP, and structured output.",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    "import": "./dist/index.esm.js",
    "require": "./dist/index.cjs.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "nodemon --watch src --exec ts-node src/index.ts",
    "start": "node dist/index.cjs.js",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "logging",
    "middleware",
    "express",
    "uuid",
    "geoip-lite",
    "express-logger",
    "firstlog"
  ],
  "author": "Aditya Singh",
  "repository": {
    "type": "git",
    "url": "https://github.com/adidecodes/firstlog"
  },
  "bugs": {
    "url": "https://github.com/adidecodes/firstlog/issues"
  },
  "homepage": "https://github.com/adidecodes/firstlog#readme",
  "dependencies": {
    "express": "^4.18.3",
    "geoip-lite": "^1.4.10",
    "source-map-support": "^0.5.21",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@types/express": "^4.17.21",
    "@types/geoip-lite": "^1.4.4",
    "@types/node": "^20.11.24",
    "@types/source-map-support": "^0.5.10",
    "@types/uuid": "^10.0.0",
    "esbuild": "^0.20.0",
    "nodemon": "^3.1.10",
    "rollup": "^2.79.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.36.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  }
}
